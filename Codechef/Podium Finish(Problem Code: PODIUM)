#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin >> T;
	
	// declare more variable
	int A,B;
	
	while(T--)
	{
	    cin >> A >> B;
	    cout << A+B << endl;
	}
	return 0;
}
