// code --->


#include <iostream>
using namespace std;

int main() {
	int t;
	int x;
	cin>>t;
	while(t--)
	{
	    cin>>x;
	    if(x<=100)
	    {
	        cout<<x<<endl;
	    }
	    else
	    {
	        
	        cout<<x-10<<endl;
	    }
	}
	return 0;
}
